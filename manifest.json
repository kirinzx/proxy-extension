{
    "manifest_version": 3,
    "name": "ProxyBy",
    "version": "1.0",
    "description": "Set your proxy and use it",
    "permissions": [
        "storage",
        "activeTab",
        "tabs",
        "proxy",
        "webRequest",
        "webRequestAuthProvider"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/24.png",
        "48": "images/48.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}